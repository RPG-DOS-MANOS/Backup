{
  "env": {
    "es6": true,
    "es2022": true
  },
  "globals": {
    "args": true,
    "AAhelpers": true,
    "Ray": true,
    "MeasuredTemplate": true,
    "MeasuredTemplateDocument": true,
    "scope": true,
    "workflow": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest",
    "ecmaFeatures": {
      "globalReturn": true,
      "impliedStrict": true
    }
  },
  "rules": {
    "no-console": "off",
    "no-await-in-loop": "off",
    "default-case": "off"
  }
}
